{
  "name": "@endezz/eslint-config",
  "type": "module",
  "version": "0.0.8",
  "description": "eslint config",
  "author": "endezz",
  "license": "MIT",
  "homepage": "https://github.com/EndeZz/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndeZz/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/EndeZz/eslint-config/issues"
  },
  "keywords": [
    "eslint-config",
    "eslint",
    "config",
    "lint",
    "react"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "tsc --watch",
    "lint": "eslint . --fix",
    "inspector": "npx @eslint/config-inspector@latest",
    "clean": "rimraf ./dist",
    "build": "pnpm run clean && tsc --declaration && esbuild \"./dist/index.js\" --minify --outfile=\"dist/index.min.js\"",
    "prepublish": "pnpm run lint && pnpm run build"
  },
  "dependencies": {
    "@antfu/eslint-config": "4.13.0",
    "eslint": "9.26.0",
    "eslint-plugin-jsx-a11y": "6.10.2"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "6.10.0",
    "@types/node": "22.15.17",
    "esbuild": "0.25.4",
    "rimraf": "6.0.1",
    "typescript": "5.8.3"
  }
}
