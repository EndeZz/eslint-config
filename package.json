{
  "name": "@endezz/eslint-config",
  "type": "module",
  "version": "0.0.9",
  "description": "eslint config",
  "author": "endezz",
  "license": "MIT",
  "homepage": "https://github.com/EndeZz/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndeZz/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/EndeZz/eslint-config/issues"
  },
  "keywords": [
    "eslint-config",
    "eslint",
    "config",
    "lint",
    "react"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "tsup --watch",
    "lint": "eslint . --fix",
    "inspector": "npx @eslint/config-inspector@latest",
    "build": "tsup",
    "prepublish": "pnpm run build && pnpm run lint"
  },
  "dependencies": {
    "@antfu/eslint-config": "4.13.0",
    "eslint": "9.26.0",
    "eslint-plugin-jsx-a11y": "6.10.2"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "6.10.0",
    "@types/node": "22.15.17",
    "tsup": "8.4.0",
    "typescript": "5.8.3"
  }
}
